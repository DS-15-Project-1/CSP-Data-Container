    "    for datetime_column in df.select_dtypes(include=['datetime64[ns]']).columns:\n",
    "        # Filter the rows based on the date range\n",
    "        filtered_df = df[(df[datetime_column] >= start_date) & (df[datetime_column] <= end_date)]\n",
